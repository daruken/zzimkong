type Order {
    id: Long
    memberId: Long
    yogurtId: Long
    type: String
    createdDate: String
}

input OrderInput {
    memberId: Long
    yogurtId: Long
    type: String
}

type OrderResponse {
    id: Long
    memberName: String
    memberEmail: String
    yogurtName: String
    yogurtType: String
    yogurtPrice: Long
    type: String
    createdDate: String
}

type MemberResponse {
    name: String,
    email: String
}
