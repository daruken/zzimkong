scalar Long

type Member {
    id: String
    name: String
    password: String
    createdDate: String
    updatedDate: String
}

input MemberInput {
    name: String
    password: String
}

type Query {
    findMembers(limit: Long, offset: Long): [Member]
}

type Mutation {
    createMember(member: MemberInput): Member
    deleteMember(memberId: String): Boolean
}
